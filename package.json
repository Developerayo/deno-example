{
    "name": "denoexample",
    "version": "1.5.0",
    "description": "Getting up to speed with Deno",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Shodipo Ayomide <shodipovi@gmail.com>",
    "license": "MIT",
    "homepage": "https://github.com/Developerayo/deno-example",
    "repository": {
      "type": "git",
      "url": "git@github.com:Developerayo/deno-example.git"
    },
    "files": [
      "dist"
    ],
    "scripts": {
      "prebuild": "rimraf dist",
      "build": "tsc --project src/tsconfig.json",
      "postbuild": "cpy src '!**/*.ts' dist && cpy src/runtime.ts src/dev-server.ts dist",
      "now-build": "next build",
      "prepublishOnly": "npm run build"
    }
  }